{
  "name": "example",
  "private": true,
  "version": "0.0.0",
  "main": "dist/main.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn -s clean && yarn -s generate && tsc",
    "style": "prettier --write 'src/**/*'",
    "dev": "ts-node-dev --tree-kill --no-notify --respawn --transpile-only src/main.ts",
    "generate": "yarn -s generate:prisma && yarn -s generate:nexus",
    "generate:prisma": "prisma2 generate",
    "generate:nexus": "NODE_ENV=development ts-node --transpile-only src/schema",
    "data:inject": "ts-node prisma/fake-data.ts"
  },
  "dependencies": {
    "@prisma/photon": "^2.0.0-preview019",
    "@types/bcrypt": "^3.0.0",
    "@types/jsonwebtoken": "^8.3.6",
    "bcrypt": "^3.0.7",
    "csv-parse": "^4.8.5",
    "csv-parser": "^2.3.2",
    "graphql": "^14.5.8",
    "graphql-shield": "^7.0.7",
    "graphql-yoga": "^1.18.3",
    "jsonwebtoken": "^8.5.1",
    "nexus": "^0.12.0-rc.5",
    "nexus-prisma": "^0.7.0-next.1"
  },
  "devDependencies": {
    "@types/faker": "^4.1.9",
    "@types/ws": "^7.2.0",
    "faker": "^4.1.0",
    "prettier": "^1.19.1",
    "prisma2": "2.0.0-preview019",
    "ts-node": "^8.6.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
