{
  "name": "sponsorenlauf-server",
  "version": "0.1.0",
  "description": "the server for the sponsorenlauf management website",
  "scripts": {
    "setup": "prisma migrate save --experimental && prisma migrate up --experimental && prisma generate && cd prisma && ts-node seed.ts",
    "studio": "prisma studio --experimental",
    "build": "npm -s run generate && tsc",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "dev:typecheck": "tsc --noEmit --watch",
    "dev": "LOG=trace APP_SECRET=test ts-node-dev --no-notify --respawn --transpile-only src/app",
    "seed": "ts-node prisma/seed"
  },
  "keywords": [],
  "author": "Moritz Hedtke <Moritz.Hedtke@t-online.de>",
  "license": "AGPL-3.0",
  "dependencies": {
    "@nexus/schema": "^0.17.0",
    "@prisma/client": "^2.10.1",
    "apollo-server-errors": "^2.4.2",
    "apollo-server-express": "^2.19.0",
    "bcrypt": "^5.0.0",
    "cookie": "^0.4.1",
    "cookie-parser": "^1.4.5",
    "csv-parse": "^4.12.0",
    "cuid": "^2.1.8",
    "dotenv": "^8.2.0",
    "express": "^5.0.0-alpha.8",
    "flat": "^5.0.2",
    "graphql": "^15.4.0",
    "graphql-middleware": "^4.0.2",
    "graphql-scalars": "^1.4.1",
    "graphql-shield": "^7.4.0",
    "nexus-plugin-prisma": "^0.23.1"
  },
  "resolutions": {
    "typescript": "4.1.0-dev.20200902"
  },
  "devDependencies": {
    "@prisma/cli": "^2.10.1",
    "@types/bcrypt": "^10.0.28-alpha",
    "@types/cookie": "^0.4.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.8",
    "@types/flat": "^5.0.1",
    "@types/node": "^14.14.35",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.1.0-dev.20201021"
  }
}
