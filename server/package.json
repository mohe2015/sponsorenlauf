{
  "name": "salty-sheep",
  "license": "UNLICENSED",
  "dependencies": {
    "@prisma/cli": "^2.0.0-beta.1",
    "@types/bcrypt": "^10.0.28-alpha",
    "@types/jsonwebtoken": "^8.3.8",
    "bcrypt": "^4.0.1",
    "csv-parse": "^4.8.8",
    "graphql": "^15.0.0",
    "graphql-middleware": "^4.0.2",
    "graphql-shield": "^7.2.2",
    "jsonwebtoken": "^8.5.1",
    "nexus": "file:.yalc/nexus",
    "nexus-plugin-prisma": "^0.6.0",
    "subscriptions-transport-ws": "^0.9.16",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "format": "npx prettier --write './**/*.{ts,md}' '!./prisma/**/*.md'",
    "seed": "prisma2 migrate save --experimental --create-db --name init && yarn prisma2 migrate up --experimental --verbose && ts-node prisma/seed.ts",
    "dev": "APP_SECRET=test LOG_LEVEL=info DEBUG=true nexus dev",
    "build": "nexus build",
    "db": "nexus db ui",
    "start": "node node_modules/.build"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "husky": "^4.2.3",
    "prettier": "^2.0.2",
    "pretty-quick": "^2.0.1"
  }
}
