{
  "name": "sponsorenlauf-server",
  "version": "0.1.0",
  "description": "the server for the sponsorenlauf management website",
  "scripts": {
    "setup": "prisma migrate dev --name init && cd prisma && ts-node seed.ts",
    "studio": "prisma studio --experimental",
    "build": "npm -s run generate && tsc",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "dev:typecheck": "tsc --noEmit --watch",
    "dev": "LOG=trace APP_SECRET=test ts-node-dev --no-notify --respawn --transpile-only src/app",
    "seed": "ts-node prisma/seed"
  },
  "keywords": [],
  "author": "Moritz Hedtke <Moritz.Hedtke@t-online.de>",
  "license": "AGPL-3.0",
  "dependencies": {
    "@prisma/client": "^2.19.0",
    "apollo-server-errors": "^2.4.2",
    "apollo-server-express": "^2.21.1",
    "bcrypt": "^5.0.1",
    "cookie": "^0.4.1",
    "cookie-parser": "^1.4.5",
    "csv-parse": "^4.15.3",
    "cuid": "^2.1.8",
    "dotenv": "^8.2.0",
    "express": "^5.0.0-alpha.8",
    "flat": "^5.0.2",
    "graphql": "^15.5.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-middleware": "^6.0.5",
    "graphql-scalars": "^1.9.0",
    "graphql-shield": "^7.5.0",
    "nexus": "^1.0.0"
  },
  "resolutions": {
    "typescript": "4.1.0-dev.20200902"
  },
  "devDependencies": {
    "@prisma/cli": "^2.19.0",
    "@types/bcrypt": "^10.0.28-alpha",
    "@types/cookie": "^0.4.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.11",
    "@types/flat": "^5.0.1",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/node": "^14.14.35",
    "prisma": "^2.19.0",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.3"
  }
}
