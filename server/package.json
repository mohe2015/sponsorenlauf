{
  "name": "example",
  "private": true,
  "version": "0.0.0",
  "main": "dist/main.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn -s clean && yarn -s generate && tsc",
    "style": "prettier --write 'src/**/*'",
    "dev": "ts-node-dev --tree-kill --no-notify --respawn --transpile-only src/main.ts",
    "generate": "yarn -s generate:prisma && yarn -s generate:nexus",
    "generate:prisma": "prisma2 generate",
    "generate:nexus": "NODE_ENV=development ts-node --transpile-only src/schema",
    "data:inject": "ts-node prisma/fake-data.ts"
  },
  "dependencies": {
    "@prisma/client": "^2.0.0-preview022",
    "@types/bcrypt": "10.0.28-alpha",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/ws": "^7.2.2",
    "apollo-server": "^2.10.1",
    "bcrypt": "^4.0.1",
    "bcryptjs": "^2.4.3",
    "csv-parse": "^4.8.7",
    "csv-parser": "^2.3.2",
    "graphql": "^14.6.0",
    "graphql-middleware": "^2.0.0",
    "graphql-shield": "^7.0.14",
    "jsonwebtoken": "^8.5.1",
    "nexus": "^0.12.0-rc.13",
    "nexus-prisma": "^0.10.0"
  },
  "devDependencies": {
    "@types/faker": "^4.1.10",
    "faker": "^4.1.0",
    "prettier": "^1.19.1",
    "prisma2": "2.0.0-preview022",
    "ts-node": "^8.6.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
