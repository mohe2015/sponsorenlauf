datasource db {
  provider = "sqlite"
  url = "***"
}

generator prisma_client {
  provider = "prisma-client-js"
}

model User {
  id            String   @default(cuid()) @id
  name          String   @unique
  password      String
  role          UserRole
  createdRounds Round[]
}

enum UserRole {
  ADMIN
  TEACHER
  VIEWER
}

model Student {
  id          String @default(cuid()) @id
  startNumber Int    @unique
  name        String @unique
  class       String
  grade       Int
  rounds      Round[]
}

model Round {
  id        String   @default(cuid()) @id
  student   Student
  createdBy User
  time      Int
}