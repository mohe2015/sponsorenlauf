{
  "name": "client2",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^7.20.0",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.5",
    "@testing-library/user-event": "^12.0.11",
    "apollo-link": "^1.2.14",
    "apollo-link-ws": "^2.0.0-beta.0",
    "bootstrap": "git+https://github.com/twbs/bootstrap.git",
    "bootstrap-icons": "^1.0.0-alpha5",
    "eslint": "^7.4.0",
    "graphql": "^15.3.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-bootstrap": "^1.2.1",
    "react-dom": "^16.13.1",
    "react-relay": "^9.1.0",
    "react-router-dom": "^6.0.0-beta.0",
    "react-scripts": "^3.4.1",
    "subscriptions-transport-ws": "^0.9.17",
    "typescript": "^4.0.0-dev.20200711",
    "webpack": "^5.0.0-beta.22"
  },
  "scripts": {
    "watch:react": "react-scripts start",
    "build:react": "react-scripts build",
    "build": "npm-run-all --sequential build:relay build:react",
    "watch": "npm-run-all --parallel watch:relay watch:react",
    "test": "npm-run-all --sequential build:relay test:react",
    "test:react": "react-scripts test --passWithNoTests",
    "build:relay": "relay-compiler --src ./src --schema ../server/src/schema/generated/schema.graphql",
    "watch:relay": "relay-compiler --src ./src --schema ../server/src/schema/generated/schema.graphql --watch"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "babel-plugin-relay": "^9.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "prettier": "2.0.5",
    "pretty-quick": "^2.0.1",
    "relay-compiler": "^9.1.0",
    "relay-config": "^9.1.0"
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "relay"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
