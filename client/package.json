    "@fortawesome/react-fontawesome": "^0.1.11",
    "@material-ui/core": "^5.0.0-alpha.10",
    "@material-ui/lab": "^5.0.0-alpha.10",
    "@material-ui/system": "^5.0.0-alpha.6",
    "@svgr/webpack": "5.4.0",
    "@testing-library/jest-dom": "^5.11.2",
