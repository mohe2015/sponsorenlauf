{
  "name": "client2",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^6.15.0",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^10.0.0",
    "apollo-link": "^1.2.13",
    "apollo-link-ws": "^2.0.0-beta.0",
    "bootstrap": "git+https://github.com/twbs/bootstrap.git",
    "bootstrap-icons": "^1.0.0-alpha2",
    "eslint": "^6.8.0",
    "graphql": "^14.6.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.0",
    "react-bootstrap": "^1.0.0-beta.17",
    "react-dom": "^16.13.0",
    "react-relay": "^9.0.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.0",
    "subscriptions-transport-ws": "^0.9.16",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0"
  },
  "scripts": {
    "watch:react": "react-scripts start",
    "build:react": "react-scripts build",
    "build": "npm-run-all --sequential build:relay build:react",
    "watch": "npm-run-all --parallel watch:relay watch:react",
    "test": "npm-run-all --sequential build:relay test:react",
    "test:react": "react-scripts test",
    "build:relay": "relay-compiler --src ./src --schema ../server/generated/schema.graphql",
    "watch:relay": "relay-compiler --src ./src --schema ../server/generated/schema.graphql --watch"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-relay": "^9.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "prettier": "1.19.1",
    "pretty-quick": "^2.0.1",
    "relay-compiler": "^9.0.0",
    "relay-config": "^9.0.0"
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "relay"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
